<h1>Listing global_configs</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @global_configs.each do |global_config| %>
      <tr>
        <td><%= global_config.key %></td>
        <td><%= global_config.value %></td>
        <td><%= link_to 'Show', global_config %></td>
        <td><%= link_to 'Edit', edit_global_config_path(global_config) %></td>
        <td><%= link_to 'Destroy', global_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Global config', new_global_config_path %>
